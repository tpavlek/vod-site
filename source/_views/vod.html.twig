{# If this is a type being generated by sculpin, then page will equal our expected vod object #}
{% if vod is not defined %}
    {% set vod = page %}
{% endif %}

{% extends 'default' %}

{% block content_wrapper %}
    {% if "Jeopardy" in vod.tournaments %}
        {% include 'vod_jeopardy.html.twig' with { 'vod':vod } %}
    {% else %}

        {% include 'vod_instance.html.twig' with {'vod':vod} %}
    {% endif %}


{% endblock %}

